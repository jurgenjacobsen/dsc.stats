{
  "name": "dsc.stats",
  "version": "3.0.0",
  "description": "A easy to use statistics package mainly for discord bots",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "docs": "typedoc --theme minimal --out docs src/",
    "pretty": "prettier --config .prettierrc \"./src/**/*.ts\"  \"./lib/**/*.js\" --write",
    "build": "npm run tsc && npm run pretty && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurgenjacobsen/dsc.stats.git"
  },
  "keywords": [
    "discord",
    "stats",
    "statistics",
    "dsc"
  ],
  "author": "jurgenjacobsen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jurgenjacobsen/dsc.stats/issues"
  },
  "homepage": "https://github.com/jurgenjacobsen/dsc.stats#readme",
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "dsc.db": "^3.0.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.1"
  }
}
